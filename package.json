{
    "name": "react-typescript-boilerplate",
    "version": "1.1.0",
    "description": "React Typescript and Styled Components boilerplate powered by Webpack 5",
    "license": "MIT",
    "author": "Nakane",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "profile": "cross-env NODE_ENV=production webpack --profile --json > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
        "start": "cross-env DEV_SERVER=true NODE_ENV=development webpack serve",
        "lint": "eslint --ext .ts,.tsx src/ --cache --fix",
        "lint:fix": "eslint --ext .ts,.tsx src/ --cache --fix",
        "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "prepare": "husky install"
    },
    "repository": {
        "url": "https://github.com/carlosnakane/react-typescript-boilerplate"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@babel/core": "^7.18.9",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-export-default-from": "^7.18.9",
        "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
        "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
        "@babel/plugin-proposal-throw-expressions": "^7.18.6",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.18.9",
        "@babel/preset-env": "^7.18.9",
        "@babel/preset-react": "^7.18.6",
        "@babel/register": "^7.18.9",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
        "@svgr/webpack": "^6.3.1",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@types/jest": "^28.1.6",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/styled-components": "^5.1.25",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "babel-loader": "^8.2.5",
        "babel-plugin-styled-components": "^2.0.7",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "core-js": "^3.24.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.20.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.3.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.6.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-webpack-plugin": "^3.2.0",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "html-loader": "^4.1.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.1",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "lint-staged": "^13.0.3",
        "mini-css-extract-plugin": "^2.6.1",
        "prettier": "^2.7.1",
        "react-refresh": "^0.14.0",
        "regenerator-runtime": "^0.13.9",
        "terser-webpack-plugin": "^5.3.3",
        "ts-jest": "^28.0.7",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.4",
        "webpack": "^5.74.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.9.3",
        "webpack-merge": "^5.8.0"
    }
}
